    "# Get probability for range of values\n",
    "start = LDV[\"K34a\"].mean()-5 # Start of the range\n",
    "end = LDV[\"K34a\"].mean()+5    # End of the range\n",
    "N = 100    # Number of evaluation points \n",
    "step = (end - start) / (N - 1)  # Step size\n",
    "x = np.linspace(start, end, N)[:, np.newaxis]  # Generate values in the range\n",
    "kd_vals = np.exp(kd.score_samples(np.array(LDV.sort_values(by=\"K34a\",ascending=True)[\"K34a\"]).reshape(-1, 1)))  # Get PDF values for each x\n",
    "probability = np.sum(kd_vals * step)  # Approximate the integral of the PDF\n",
    "print(probability) # log like probability"
   ]
