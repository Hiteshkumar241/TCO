   "source": [
    "import numpy as np\n",
    "def plotHistogram(data, column, bins,cum=False, xlabel=None, ylabel=None,titletext=None):\n",
    "    dataframe=data#pd.DataFrame.from_dict(d[-1])\n",
    "    #dataframe=dataframe.reset_index()\n",
    "    weights = np.ones_like(dataframe[column]/dataframe[column].sum())/float(len(dataframe[column])) \n",
    "    \n",
    "    fig, ax = plt.subplots(figsize=(8,6))\n",
    "\n",
    "    x = (dataframe[column])\n",
    "\n",
    "    # the histogram of the data\n",
    "    n, bins, patches = plt.hist(x, bins,normed=0,weights=weights, facecolor='blue', alpha=0.5,cumulative=cum)\n",
    "\n",
    "    plt.xlabel(xlabel)\n",
    "    plt.ylabel(ylabel)\n",
    "    plt.title(titletext)\n",
    "    plt.grid(True)\n",
    "\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    "
   ]
  },
